@{
    ViewData["Title"] = "Home Page";
}
<div>
    <div class="SameRow">
        <h3>WorkSheet</h3>
        <p1>
            <button class="QuestionInteface" onclick="saveData()" id="Save">Save</button>
            <button class="QuestionInteface" id="remove">Remove</button>
        </p1>
    </div>
    <form id="worksheetForm">
        <label for="Title">Title:</label>
        <input type="text" id="Title" name="Title"><br><br>
        <label for="FinalMessage">Final Message:</label>
        <input type="text" id="FinalMessge" name="FinalMessge"><br><br>
    </form>
    <div class="SameRow">
        <p1>
            <select name="Topics" id="QuestionInterfaceSelctor">
                <option value="A">Topics</option>
                <option value="B">B</option>
                <option value="-">Other</option>
            </select>
            <select name="Skills" id="QuestionInterfaceSelctor">
                <option value="A">SKILL</option>
                <option value="B">B</option>
                <option value="-">Other</option>
            </select>
        </p1>
        <select name="Type" id="QuestionInterfaceSelctor">
            <option value="A">TYPE</option>
            <option value="B">B</option>
            <option value="-">Other</option>
        </select>
    </div>
    <br />
    <h3>Questions</h3>
    <hr />
    <div id="MainQuestionDivName"></div>
    <button type="button" class="QuestionInteface" id="AddQuestionInterface">Add</button>
</div>
